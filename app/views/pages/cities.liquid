---
title: Cities
published: true
listed: true
---

{% extends parent %}

{% block 'main' %}
<div class="gradiente">  </div>
<section class="page">
  <div class="sticky">
    <div class="frststicky" >
        <div class="sprites-social top"></div>
        TOP
    </div>
        <div class="scndsticky">
        <div class="sprites-social share" style="margin: 8px 30% !important;" ></div>
        SHARE
        </div>
  </div>
    <section class="row">
    {% editable_text title-cities %}
      <h1 class="main-title">Participate with thousands of entrepreneurs across the globe <br/>
        During two of the most innovative weekends in November</h1>
    {% endeditable_text %}
      </section>
      
      <div class="row slct-content">
        <div class="large-offset-1 large-10"> 
          <div class="select">
            <p>City Search:</p>
            <select name="sasa" id="city" class="chosen-select" onchange="window.location.href=this.value;">
              <option value="">Type city name</option>
              {% for event in contents.events %}
                  <option value="{{event.url}}">{{ event.city }}</option>
                  {% assign date_show = '' %}
              {% endfor %}
            </select>
          </div>
          <div class="select">
            <p>Filter by:</p>
            <select name="sasa  " id="country" class="chosen-select">
              <option value="">Select a region</option>
                {% for region in contents.regions %}
                  <option value="{{region.acronym}}">{{ region.country }}</option>
                {% endfor %}
            </select>
          </div>
          <div class="select">
            <p><br /></p>
            <select name="sasa" id="date" class="chosen-select">
              <option value="">Select a date range</option>
                {% for event in contents.events %}
                  {% if date_show != event.date %}
                      <option value="{{event.date || localized_date: '%d/%m/%Y'}}">{{ event.date || localized_date: '%d/%m/%Y' }}</option>
                  {% endif %}
                  {% assign date_show = event.date %}
                {% endfor %}
            </select>
          </div>
        </div>
      </div>
      <div class="row m-btm" data-name="us">
        <div class="large-offset-2 large-8 columns">
          <div class=" cty-ttl">  
            <div class="sprites usa"></div> <div class="ctn-txt">UNITED STATES</div>
          </div>
           {% with_scope country: 'UNITED STATES' %}
              {% assign count = '1' %}
                {% for event in contents.events %}
                  {% if count == '1' %} <div class="row cty"> {% endif %}
                    <div class="large-4 medium-4 small-4 columns cty-ft" data-name="{{ event.city }}">
                      <span>
                        <a class="cty-url" href="{{event.url}}">{{ event.city }}</a>
                      </span>
                      <input class="cty-date" type="hidden" value="{{event.date || localized_date: '%d/%m/%Y'}}">
                    </div>
                    
                  {% if count == '3' or forloop.length == forloop.index %} </div> {% assign count = '0' %} {% endif %}
                  {% capture count %}{{ count | plus:'1' }}{% endcapture %}
                {% endfor %}
            {% endwith_scope %}
            
        </div> 
      </div>
